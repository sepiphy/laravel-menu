{
    "name": "sepiphy/laravel-menu",
    "description": "The sepiphy/laravel-menu package",
    "keywords": [
        "sepiphy", "laravel", "laravel-menu", "menu"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laravel-menu",
    "support": {
        "issues": "https://github.com/sepiphy/laravel-menu/issues",
        "source": "https://github.com/sepiphy/laravel-menu"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "illuminate/database": "^5.8 | ^6.0 | ^7.0 | ^8.0",
        "illuminate/support": "^5.8 | ^6.0 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17",
        "phpstan/phpstan": "^0.12.85"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Menu\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Menu\\MenuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
